{
  "name": "@computost/website",
  "packageManager": "pnpm@10.10.0",
  "private": true,
  "type": "module",
  "scripts": {
    "apply-env-to-wrangler-config": "tsx ./workers/apply-env-to-wrangler-config.ts",
    "build": "react-router build",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "dotenv -- drizzle-kit migrate",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "lint": "eslint",
    "start": "wrangler dev",
    "test": "vitest",
    "test:e2e": "playwright test",
    "typecheck": "react-router typegen && tsc -b"
  },
  "dependencies": {
    "@react-router/node": "^7.5.0",
    "@react-router/serve": "^7.5.0",
    "drizzle-orm": "^0.41.0",
    "isbot": "^5.1.25",
    "radix-ui": "^1.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.11",
    "@cloudflare/workers-types": "^4.20250406.0",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.1",
    "@react-router/dev": "^7.5.0",
    "@tailwindcss/vite": "^4.1.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/wait-on": "^5.3.4",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "~0.30.6",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "execa": "^9.5.2",
    "get-port": "^7.1.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "lefthook": "^1.11.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.3",
    "tiny-jsonc": "^1.0.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0",
